{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "current file",
      "type": "pwa-node",
      "request": "launch",
      "args": ["${relativeFile}"],
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}",
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"]
    }
  ]
}
